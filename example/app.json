{
  "expo": {
    "name": "@tracked/health example",
    "slug": "tracked-health-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trckd.health.example",
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.background-delivery": true
      },
      "infoPlist": {
        "NSHealthShareUsageDescription": "We need access to your health data to track your steps",
        "NSHealthUpdateUsageDescription": "We need access to your health data to track your steps"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.trckd.health.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}